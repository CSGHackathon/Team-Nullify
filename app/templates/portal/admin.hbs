<img class="img-responsive" src="/assets/images/Banner.png">
<h1>Admin</h1>
{{outlet}}

<table class="table">
  <thead>
    <tr>
      <th>
      Date
      </th>
      <th>
      Time
      </th>
      <th>
      Message
      </th>
      <th>
      Phone
      </th>
      <th>
      Account Owner
      </th>
    </tr>
  </thead>
  {{log modelArray}}
  {{#each model.content.content as |message|}}
    {{log message._data}}
    <tbody>
      <tr>
        {{#each message._data as |field|}}
        {{log field}}
          <td class="">
            {{field.date}}
            {{field.time}}
            {{field.body}}
            {{field.phone}}
            {{field.acct_name}}
          </td>
        {{/each}}
      </tr>
    </tbody>
  {{/each}}
</table>